<?xml version="1.0" encoding="UTF-8"?>
<!-- src/com/controller/WebApplicationContext에서 파싱 -->
<!-- 
	SAX방식에서 태그를 읽는 메소드들
	startDocument() : xml이 시작이 되었음을 알림
 -->
<beans><!-- startElement() -->
	<!-- main.do가 들어왔을 때 매핑할 클래스 => MainModel -->
	<bean id="main" class="com.model.MainModel"/><!-- startElement(), endElement() -->
	<bean id="idcheck" class="com.model.IdCheckModel"/>
</beans><!-- endElement(), endDocument() -->